{
    "name": "arubacao/php-cs-fixer-config",
    "description": "Preconfigured PHP Coding Standards Fixer - rule sets for Laravel projects/packages & plain PHP",
    "keywords": [
        "Preconfigured PHP Coding Standards Fixer",
        "Code Styling"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "arubacao",
            "email": "arubacao@gmail.com"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Arubacao\\PhpCsFixer\\": "src"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix ./ --config=recommended.php_cs -vv"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
