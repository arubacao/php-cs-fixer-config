{
  "name": "arubacao/php-cs-fixer-config",
  "type": "library",
  "description": "Preconfigured PHP Coding Standards Fixer for Laravel & plain PHP",
  "keywords": [
    "Preconfigured PHP Coding Standards Fixer",
    "Code Styling",
    "Laravel Preset",
    "Laravel Rule Set",
    "Laravel Code Style"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Lass",
      "homepage": "https://c-lass.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Arubacao\\PhpCsFixer\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix ./ --config=recommended.php_cs -vv"
  }
}
